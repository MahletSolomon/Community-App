<UserControl x:Class="MainProject.Componets.IconButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MainProject.Componets"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
      
    </UserControl.Resources>
    <Button BorderThickness="0" Margin="5 8" FontSize="15" Padding="0 4" Content="{Binding ButtonContent, RelativeSource={RelativeSource AncestorType=UserControl}}" Style="{StaticResource ButtonAbiy}" Command="{Binding ButtonCommand, RelativeSource={RelativeSource AncestorType=UserControl}}"/>

</UserControl>
